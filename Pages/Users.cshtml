@page
@model CRM.Pages.Users
@{
    ViewData["Title"] = "Users";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Role</th>
        
    </tr>

    @foreach(var user in Model.UserList)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>@user.Phone</td>
            <td>@user.Role</td>
        </tr>
    }
</table>