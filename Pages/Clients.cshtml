@page
@using CRM.Models
@model CRM.Pages.Clients
@{
    ViewData["Title"] = "Clients";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Phone</th>

        
    </tr>

    @foreach(var client in Model.ClientList)
    {
        <tr>
            <td>@client.Id</td>
            <td>@client.FirstName</td>
            <td>@client.LastName</td>
            <td>@client.Email</td>
            <td>@client.Phone</td>
        </tr>
    }
</table>